{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "ext-curl": "*",
        "ext-dom": "*",
        "ext-gd": "*",
        "ext-libxml": "*",
        "ext-simplexml": "*",
        "barryvdh/laravel-dompdf": "^2.0",
        "doctrine/dbal": "^3.0",
        "giggsey/libphonenumber-for-php": "^8.13",
        "guzzlehttp/guzzle": "^7.2",
        "haringsrob/livewire-datepicker": "dev-main",
        "illuminate/cache": "^9.0",
        "illuminate/http": "^9.0",
        "illuminate/log": "^9.0",
        "kdion4891/laravel-livewire-tables": "^1.6",
        "laravel/framework": "^9.52",
        "laravel/jetstream": "^2.9",
        "laravel/sanctum": "^3.2",
        "laravel/tinker": "^2.8",
        "livewire/livewire": "^2.12",
        "maatwebsite/excel": "^3.1",
        "mediconesystems/livewire-datatables": "^0.10.3",
        "phpmailer/phpmailer": "^6.8",
        "phpoffice/phpspreadsheet": "^1.29",
        "phpseclib/phpseclib": "3.0",
        "phpsysinfo/phpsysinfo": "^3.4",
        "power-components/livewire-powergrid": "^1.2",
        "psr/simple-cache": "1.0",
        "smalot/pdfparser": "^2.12",
        "spatie/laravel-permission": "^6.20",
        "stillat/blade-parser": "^1.10",
        "te7a-houdini/laravel-trix": "^2.0",
        "wire-elements/modal": "^1.0",
        "webklex/laravel-imap": "^4.0"
    },
    "replace": {
        "carbonphp/carbon-doctrine-types": "*"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^6.1",
        "phpunit/phpunit": "^9.5.10",
        "spatie/laravel-ignition": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/",
            "PhpSysInfo\\": "vendor/phpsysinfo/phpsysinfo/src/"
        },
        "files": [
            "app/Helpers/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "minimum-stability": "dev",
        "prefer-stable": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
